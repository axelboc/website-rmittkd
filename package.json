{
  "private": true,
  "name": "website-rmittkd",
  "version": "3.0.0",
  "author": "Axel Bocciarelli",
  "description": "Website of the RMIT ITF Taekwon-Do club.",
  "license": "MIT",
  "homepage": "https://rmittkd.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelboc/website-rmittkd"
  },
  "engines": {
    "node": "^8.11.3",
    "yarn": "*"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:local": "set NODE_ENV=development && yarn build && yarn serve",
    "build:prod": "set NODE_ENV=production && yarn build && yarn serve",
    "serve": "gatsby serve -p 8000",
    "lint": "run-p lint:*",
    "lint:js": "eslint \"*.js\" \"src/**/*.js\"",
    "lint:js:check": "eslint --print-config . | eslint-config-prettier-check",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:check": "stylelint-config-prettier-check",
    "lint:pretty": "prettier --check \"**/*.{js,css,json,md}\""
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-problems": "^2.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^3.1.0",
    "gatsby": "^2.3.32",
    "gatsby-plugin-netlify-cms": "^4.0.0",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-sitemap": "^2.0.12",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.18",
    "husky": "^2.1.0",
    "lodash": "^4.17.11",
    "netlify-cms-app": "^2.9.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "romanize": "^1.1.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0"
  }
}
